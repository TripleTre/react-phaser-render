{
  "name": "react--phaser",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "node build/dev-server.js",
    "test": "BABEL_ENV=test jest"
  },
  "repository": "https://github.com/huaguhzheng/react--phaser.git",
  "author": "赵巍 <huaguzheng2007@126.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^21.1.8",
    "chai": "^4.1.2",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.16.2",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "http-proxy-middleware": "^0.17.4",
    "jest": "^21.2.1",
    "nightwatch": "^0.9.16",
    "nightwatch-html-reporter": "^2.0.4",
    "ts-loader": "^3.1.1",
    "typescript": "^2.6.1",
    "webpack": "^3.8.1",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.20.0",
    "webpack-merge": "^4.1.1"
  },
  "dependencies": {
    "expose-loader": "^0.7.4",
    "fbjs": "^0.8.16",
    "lodash": "^4.17.4",
    "object-assign": "^4.1.1",
    "p2": "^0.7.1",
    "phaser-ce": "^2.9.2",
    "pixi": "^0.3.1",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-reconciler": "^0.6.0",
    "react-router": "^4.2.0"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/__test__/jest.setup.js"
    ],
    "transform": {
      ".*": "<rootDir>/__test__/jest.preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy",
      "^react-phaser-render$": "<rootDir>/src"
    },
    "globals": {
      "UcEnv": "testing"
    }
  }
}
